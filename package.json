{
  "name": "comfyui-api",
  "version": "1.8.4",
  "description": "Wraps comfyui to make it easier to use as a stateless web service",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "mocha --require ts-node/register --timeout=0 test/*.spec.ts",
    "build": "tsc",
    "build-binary": "./build-binary",
    "postinstall": "npm install --cpu=wasm32 sharp"
  },
  "author": "Shawn Rushefsky",
  "license": "MIT",
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.26.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.12.7",
    "@types/ws": "^8.5.13",
    "@yao-pkg/pkg": "^6.1.0",
    "earl": "^1.3.0",
    "minimist": "^1.2.8",
    "mocha": "^11.0.1",
    "ts-node": "^10.9.2"
  },
  "bin": {
    "comfyui-api": "dist/src/index.js"
  },
  "dependencies": {
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "fastify": "^4.26.2",
    "fastify-type-provider-zod": "^2.0.0",
    "sharp": "^0.34.2",
    "typescript": "^5.4.5",
    "undici": "^7.2.3",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "pkg": {
    "targets": [
      "node20-linux-x64"
    ],
    "outputPath": "bin",
    "public": true
  },
  "engines": {
    "node": ">=20.18.1"
  }
}
